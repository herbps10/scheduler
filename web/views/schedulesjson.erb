{
	"sections": [
		<%= course_json_list(@scheduler.sections) %>
	],

	"schedules": [
	<% @scheduler.schedules.each_with_index do |schedule, schedule_index| %>
		{
			"courses": [
				<%= course_json_list(schedule.sections) %>
			],
			"conflicts": [
				<%= course_json_list(schedule.conflicts) %>
			]
		}<%= "," if schedule_index < @scheduler.schedules.length - 1 %>
	<% end %>
	]
}
