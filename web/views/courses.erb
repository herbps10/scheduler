{
	"departments": [
		<% @data.departments.each_index do |department_index| %>
			<% department = @data.departments[department_index] %>
			{
				"title": "<%= department.title %>",
				"courses": [
					<% department.courses.each_index do |course_index| %>
						<% course = department.courses[course_index] %>
						{
							"title": "<%= course.title %>",
							"sections": [
							<% course.sections.each_index do |section_index| %>
								<% section = course.sections[section_index] %>
								{
									"crn": <%= section.crn %>
								}<%= "," if section_index < course.sections.length - 1 %>
							<% end %>
							]
						}<%= "," if course_index < department.courses.length - 1 %>
					<% end %>
				]
			}<%= "," if department_index < @data.departments.length - 1 %>
		<% end %>
	]
}
